{
    "name": "kilip/behat-laravel-extension",
    "description": "Provides more extensive features to Laravel Doctrine",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
            ],
            "aliases": {}
        }
    },
    "autoload": {
        "psr-4": {
            "Behat\\LaravelExtension\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\DummyPackage\\": "./fixtures/test-package",
            "App\\": "./fixtures/laravel"
        }
    },
    "scripts": {
        "behat": "./vendor/bin/behat --colors -fprogress",
        "phpstan": "./vendor/bin/phpstan analyse --ansi",
        "phpspec": "./vendor/bin/phpspec --ansi run -fpretty",
        "phpcs": "./vendor/bin/php-cs-fixer fix --ansi",
        "coverage": "@phpunit --coverage-html=build/coverage",
        "verify": [
            "@phpcs --dry-run --diff",
            "@phpunit",
            "@phpstan"
        ]
    },
    "suggest": {
        "symfony/yaml": "To use doctrine yaml config"
    },
    "require": {
        "php": ">=7.2",
        "friends-of-behat/mink-browserkit-driver": "^1.3|^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "illuminate/support": "^5.8|^6.0|^7.20"
    },
    "require-dev": {
        "fideloper/proxy": "^4.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "laravel/framework": "^5.8|^6.0|^7.0",
        "laravel/tinker": "^1.0.10|^2.4",
        "orchestra/testbench-core": "^3.9.4|^4.8|^5.3",
        "phpspec/phpspec": "^6.2",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.30",
        "phpstan/phpstan-symfony": "^0.12.7",
        "phpunit/phpunit": "^8.5"
    }
}
